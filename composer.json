{
    "name": "kientv/omnipay-vnpay",
    "type": "library",
    "description": "Thư viện hổ trợ tích hợp cổng thanh toán VNPay.",
    "keywords": [
        "phpviet",
        "vnpay",
        "payment",
        "vietnam-payment",
        "payment-gateway",
        "vietnam-payment-gateway"
    ],
    "homepage": "https://github.com/kientv/omnipay-vnpay",
    "license": "MIT",
    "authors": [
        {
            "name": "Vuong Xuong Minh",
            "email": "vuongxuongminh@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "omnipay/common": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\VNPay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\VNPay\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "\"vendor/bin/phpunit\""
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
	}
}
